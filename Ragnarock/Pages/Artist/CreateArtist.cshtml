@page
@model Ragnarock.Pages.Artist.CreateArtistModel
@{
    ViewData["Title"] = "CreateArtist";
}
<h1>CreateArtist</h1>
 
<form method="post">

    <div class="form-group">
        <label asp-for="@Model.Artists.Id" class="control-label"> Id </label>
        <input asp-for="@Model.Artists.Id" class="form-control" />
        <span asp-validation-for="@Model.Artists.Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Artists.Name" class="control-label">Name </label>
        <input asp-for="@Model.Artists.Name" class="form-control" />
        <span asp-validation-for="@Model.Artists.Name" class="text-danger"></span>

    </div>
    <div class="form-group">
        <label asp-for="@Model.Artists.Description" class="control-label">Description</label>
        <input asp-for="@Model.Artists.Description" class="form-control" />
        <span asp-validation-for="@Model.Artists.Description" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Artists.BirthYear" class="control-label">BirthYear</label>
        <input asp-for="@Model.Artists.BirthYear" class="form-control" />
        <span asp-validation-for="@Model.Artists.BirthYear" class="text-danger"></span>
    </div>
<div class="form-group">
    <label asp-for="@Model.Artists.DeathYear" class="control-label">DeathYear</label>
    <input asp-for="@Model.Artists.DeathYear" class="form-control" />
    <span asp-validation-for="@Model.Artists.DeathYear" class="text-danger"></span>
</div>
    <div class="form-group">
        <label asp-for="@Model.Artists.ImageName" class="control-label"> ImageName </label>
        <input asp-for="@Model.Artists.ImageName" class="form-control" />
        <span asp-validation-for="@Model.Artists.ImageName" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>

